{
  "name": "frontend",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "api:generate:employee": "openapi-generator-cli generate -g typescript-angular -i http://localhost:8080/employee-service/v3/api-docs -o src/app/core/api/generated/employee --additional-properties=ngVersion=20.3.0,serviceSuffix=Api,modelSuffix=Dto,withSeparateModelsAndApi=true",
    "api:generate:project": "openapi-generator-cli generate -g typescript-angular -i http://localhost:8080/project-service/v3/api-docs -o src/app/core/api/generated/project --additional-properties=ngVersion=20.3.0,serviceSuffix=Api,modelSuffix=Dto,withSeparateModelsAndApi=true",
    "api:generate:notification": "openapi-generator-cli generate -g typescript-angular -i http://localhost:8080/notification-service/v3/api-docs -o src/app/core/api/generated/notification --additional-properties=ngVersion=20.3.0,serviceSuffix=Api,modelSuffix=Dto,withSeparateModelsAndApi=true",
    "api:generate:all": "npm run api:generate:employee && npm run api:generate:project && npm run api:generate:notification"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^20.3.0",
    "@angular/compiler": "^20.3.0",
    "@angular/core": "^20.3.0",
    "@angular/forms": "^20.3.0",
    "@angular/platform-browser": "^20.3.0",
    "@angular/router": "^20.3.0",
    "@tailwindcss/postcss": "^4.1.14",
    "leaflet": "^1.9.4",
    "postcss": "^8.5.6",
    "rxjs": "~7.8.0",
    "tailwindcss": "^4.1.14",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^20.3.3",
    "@angular/cli": "^20.3.3",
    "@angular/compiler-cli": "^20.3.0",
    "@types/jasmine": "~5.1.0",
    "@types/leaflet": "^1.9.21",
    "jasmine-core": "~5.9.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.9.2"
  }
}
